<Window x:Class="WpfApp1.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Delete" Height="350" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Grid>
            <TextBlock Text="DELTE" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="40"/>
        </Grid>
        <StackPanel  Grid.Row="1">
            <Button x:Name="YES" Content="YES" Margin="50,60" Click="YES_Click_01" />
            <Button x:Name="NO" Content="NO" Margin="50,0" Click="NO_Click_01" />
        </StackPanel>
    </Grid>
</Window>

